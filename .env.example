# Anthropic API Key
ANTHROPIC_API_KEY=your_api_key_here

# Claude model to use
MODEL_NAME=claude-3-7-sonnet-20250219

# Directory for saving output files
OUTPUT_DIR=./output

# Maximum retries for validation failures
MAX_VALIDATION_RETRIES=3

# Default number of questions to generate
DEFAULT_QUESTION_COUNT=5

# Parallel processing settings
MAX_VALIDATION_WORKERS=4
MAX_FIX_WORKERS=4
MAX_GENERATION_WORKERS=2

# API retry settings
MAX_API_RETRIES=3
API_RETRY_DELAY_SECONDS=2.0 

# Google Drive API settings
GDRIVE_CREDENTIALS_PATH=service_credentials.json